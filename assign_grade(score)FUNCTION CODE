def assign_grade(score):
  
    if score < 0 or score > 100:
        print("MGA GRADES")
    elif score >= 90:
        print("Grade: A")
    elif score >= 80:
        print("Grade: B")
    elif score >= 70:
        print("Grade: C")
    elif score >= 60:
        print("Grade: D")
    else:
        print("Grade: F")

assign_grade(98)
assign_grade(88) 
assign_grade(75)  
assign_grade(69)  
assign_grade(50)
